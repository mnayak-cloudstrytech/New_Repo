package com.spring.SpringCore;


import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.spring.SpringCoreDemo.demo.Department;
import com.spring.SpringCoreDemo.demo.Employee;
public class AppMain {

	public static void main(String[] args) {
		ApplicationContext cont=new ClassPathXmlApplicationContext("hello.xml");
		Department d1=(Department)cont.getBean("dept1");
		System.out.println("Department code is "+d1.getDeptcode());
		System.out.println("Department name is "+d1.getDeptname());
		Employee emp=d1.getEobj();
		System.out.println("Employee name is "+emp.getEmpname());
		
	}
}